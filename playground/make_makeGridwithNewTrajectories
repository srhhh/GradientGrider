a.out : PARAMETERS.o FUNCTIONS.o PHYSICS.o VARIABLES.o ANALYSIS.o ls_rmsd.o ls_rmsd_original.o mapCellData.o PHYSICSFUNCTIONS.o checkGrid.o addFrametoGrid.o addNewTrajectorytoGrid.o checkNewTrajectorywithGrid.o analyzeScatteringAngleswithMultipleGrids.o makeGridwithNewTrajectories.o
	$(FC) -o a.out -O3 -fcheck="bound" PARAMETERS.o FUNCTIONS.o PHYSICS.o VARIABLES.o ANALYSIS.o ls_rmsd.o ls_rmsd_original.o mapCellData.o PHYSICSFUNCTIONS.o checkGrid.o addFrametoGrid.o addNewTrajectorytoGrid.o checkNewTrajectorywithGrid.o analyzeScatteringAngleswithMultipleGrids.o makeGridwithNewTrajectories.o

FC = gfortran

PARAMETERS.o : PARAMETERS.f90
	$(FC) -c PARAMETERS.f90
FUNCTIONS.o : FUNCTIONS.f90
	$(FC) -c FUNCTIONS.f90
VARIABLES.o : VARIABLES.f90
	$(FC) -c VARIABLES.f90
PHYSICS.o : PHYSICS.f90
	$(FC) -c PHYSICS.f90
ANALYSIS.o : ANALYSIS.f90
	$(FC) -c ANALYSIS.f90
ls_rmsd.o : ls_rmsd.f90
	$(FC) -c ls_rmsd.f90
ls_rmsd_original.o : ls_rmsd_original.f90
	$(FC) -c ls_rmsd_original.f90
PHYSICSFUNCTIONS.o : PHYSICSFUNCTIONS.f90
	$(FC) -c PHYSICSFUNCTIONS.f90
addFrametoGrid.o : addFrametoGrid.f90
	$(FC) -c addFrametoGrid.f90
checkGrid.o : checkGrid.f90
	$(FC) -c checkGrid.f90
addNewTrajectorytoGrid.o : addNewTrajectorytoGrid.f90
	$(FC) -c addNewTrajectorytoGrid.f90
checkNewTrajectorywithGrid.o : checkNewTrajectorywithGrid.f90
	$(FC) -c checkNewTrajectorywithGrid.f90
makeGridwithNewTrajectories.o : makeGridwithNewTrajectories.f90
	$(FC) -c makeGridwithNewTrajectories.f90
mapCellData.o : mapCellData.f90
	$(FC) -c mapCellData.f90
analyzeScatteringAngleswithMultipleGrids.o : analyzeScatteringAngleswithMultipleGrids.f90
	$(FC) -c analyzeScatteringAngleswithMultipleGrids.f90
clean :
	rm *.o *.mod
