a.out : PARAMETERS.o FUNCTIONS.o PHYSICS.o VARIABLES.o ANALYSIS.o ls_rmsd.o ls_rmsd_original.o mapCellData.o PHYSICSFUNCTIONS.o checkGrid.o addFrametoGrid.o addNewTrajectorytoGrid.o checkNewTrajectorywithGrid.o analyzeScatteringAngleswithMultipleGrids.o makeGridwithNewTrajectories.o
	$(FC) -o a.out -O3 -fcheck="bound" PARAMETERS.o FUNCTIONS.o PHYSICS.o VARIABLES.o ANALYSIS.o ls_rmsd.o ls_rmsd_original.o mapCellData.o PHYSICSFUNCTIONS.o checkGrid.o addFrametoGrid.o addNewTrajectorytoGrid.o checkNewTrajectorywithGrid.o analyzeScatteringAngleswithMultipleGrids.o makeGridwithNewTrajectories.o

FC = gfortran
SOURCE = /home/kazuumi/Desktop/GradientGrider/playground/

PARAMETERS.o : $(SOURCE)PARAMETERS.f90
	$(FC) -c $(SOURCE)PARAMETERS.f90
FUNCTIONS.o : $(SOURCE)FUNCTIONS.f90
	$(FC) -c $(SOURCE)FUNCTIONS.f90
VARIABLES.o : $(SOURCE)VARIABLES.f90
	$(FC) -c $(SOURCE)VARIABLES.f90
PHYSICS.o : $(SOURCE)PHYSICS.f90
	$(FC) -c $(SOURCE)PHYSICS.f90
ANALYSIS.o : $(SOURCE)ANALYSIS.f90
	$(FC) -c $(SOURCE)ANALYSIS.f90
ls_rmsd.o : $(SOURCE)ls_rmsd.f90
	$(FC) -c $(SOURCE)ls_rmsd.f90
ls_rmsd_original.o : $(SOURCE)ls_rmsd_original.f90
	$(FC) -c $(SOURCE)ls_rmsd_original.f90
PHYSICSFUNCTIONS.o : $(SOURCE)PHYSICSFUNCTIONS.f90
	$(FC) -c $(SOURCE)PHYSICSFUNCTIONS.f90
addFrametoGrid.o : $(SOURCE)addFrametoGrid.f90
	$(FC) -c $(SOURCE)addFrametoGrid.f90
checkGrid.o : $(SOURCE)checkGrid.f90
	$(FC) -c $(SOURCE)checkGrid.f90
addNewTrajectorytoGrid.o : $(SOURCE)addNewTrajectorytoGrid.f90
	$(FC) -c $(SOURCE)addNewTrajectorytoGrid.f90
checkNewTrajectorywithGrid.o : $(SOURCE)checkNewTrajectorywithGrid.f90
	$(FC) -c $(SOURCE)checkNewTrajectorywithGrid.f90
makeGridwithNewTrajectories.o : $(SOURCE)makeGridwithNewTrajectories.f90
	$(FC) -c $(SOURCE)makeGridwithNewTrajectories.f90
mapCellData.o : $(SOURCE)mapCellData.f90
	$(FC) -c $(SOURCE)mapCellData.f90
analyzeScatteringAngleswithMultipleGrids.o : $(SOURCE)analyzeScatteringAngleswithMultipleGrids.f90
	$(FC) -c $(SOURCE)analyzeScatteringAngleswithMultipleGrids.f90
clean :
	rm *.o *.mod
