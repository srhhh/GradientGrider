a.out : f1_parameters.o f1_variables.o functions.o addCells5.o getCells.o
	$(FC) -o a.out -O3 -fcheck="bound" f1_parameters.o f1_variables.o functions.o addCells5.o getCells.o

FC = gfortran

f1_parameters.o : f1_parameters.f90
	$(FC) -c f1_parameters.f90
f1_variables.o : f1_variables.f90
	$(FC) -c f1_variables.f90
functions.o : f1_functions.f90
	$(FC) -c functions.f90
addCells5.o : addCells5.f90
	$(FC) -c addCells5.f90
getCells.o : getCells.f90
	$(FC) -c getCells.f90
clean :
	rm  getCells.o f1_parameters.o f1_parameters.mod f1_variables.o f1_variables.mod functions.o f1_functions.mod addCells5.o addframetogrid.mod
