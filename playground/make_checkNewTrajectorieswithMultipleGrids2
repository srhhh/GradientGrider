	$(FC) -fopenmp -c $(SOURCE)ANALYSIS.f90
ls_rmsd_original.o : $(SOURCE)ls_rmsd_original.f90
	$(FC) -fopenmp -c $(SOURCE)ls_rmsd_original.f90
#interactSingleGrid.o : $(SOURCE)interactSingleGrid.f90
