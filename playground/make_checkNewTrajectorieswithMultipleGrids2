a.out : DOUBLE.o PARAMETERS.o FUNCTIONS.o PHYSICS.o ANALYSIS.o VARIABLES.o ls_rmsd_original.o mapCellData.o interactMultipleGrids.o analyzeHeatMapswithMultipleGrids.o runTrajectory.o analyzeScatteringAngleswithMultipleGrids.o analyzeRMSDThresholdwithMultipleGrids.o checkNewTrajectorieswithMultipleGrids.o
	$(FC) -fopenmp -o a.out -O3 -fcheck="bound" DOUBLE.o PARAMETERS.o FUNCTIONS.o PHYSICS.o ANALYSIS.o VARIABLES.o ls_rmsd_original.o mapCellData.o interactMultipleGrids.o analyzeHeatMapswithMultipleGrids.o runTrajectory.o analyzeScatteringAngleswithMultipleGrids.o analyzeRMSDThresholdwithMultipleGrids.o checkNewTrajectorieswithMultipleGrids.o

FC = gfortran
SOURCE = /home/kazuumi/Desktop/GradientGrider/playground/

DOUBLE.o : $(SOURCE)DOUBLE.f90
	$(FC) -fopenmp -c $(SOURCE)DOUBLE.f90
PARAMETERS.o : $(SOURCE)PARAMETERS.f90
	$(FC) -fopenmp -c $(SOURCE)PARAMETERS.f90
FUNCTIONS.o : $(SOURCE)FUNCTIONS.f90
	$(FC) -fopenmp -c $(SOURCE)FUNCTIONS.f90
VARIABLES.o : $(SOURCE)VARIABLES.f90
	$(FC) -fopenmp -c $(SOURCE)VARIABLES.f90
PHYSICS.o : $(SOURCE)PHYSICS.f90
	$(FC) -fopenmp -c $(SOURCE)PHYSICS.f90
